entrypoint = "main.dart"
modules = ["dart-3.3", "nodejs-20"]

[nix]
channel="stable-24_05"

[deployment]
run = ["npm", "run", "server"]
deploymentTarget = "autoscale"

[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:2.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Main App"

[[workflows.workflow]]
name = "Main App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run server"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5002
externalPort = 5173
